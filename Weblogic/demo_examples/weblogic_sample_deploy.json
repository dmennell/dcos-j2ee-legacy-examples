{
  "id": "/hello-world/weblogic-sample-app",
  "backoffFactor": 1.15,
  "backoffSeconds": 1,
  "cmd": "env | sort && ls -lt /u01/oracle && pwd &&  wlst /u01/oracle/app-deploy.py && startWebLogic.sh",
  "container": {
    "portMappings": [
      {
        "containerPort": 8001,
        "hostPort": 0,
        "labels": {
          "VIP_0": "/weblogic-app:8001"
        },
        "protocol": "tcp",
        "servicePort": 10000,
        "name": "weblogic-management-server"
      },
      {
        "containerPort": 5556,
        "hostPort": 0,
        "labels": {
          "VIP_1": "/hello-world/weblogic-sample-app:5556"
        },
        "protocol": "tcp",
        "servicePort": 10001,
        "name": "weblogic-administration-server"
      },
      {
        "containerPort": 7001,
        "hostPort": 0,
        "labels": {
          "VIP_2": "/hello-world/weblogic-sample-app:7001"
        },
        "protocol": "tcp",
        "servicePort": 10002,
        "name": "weblogic-node-manager"
      }
    ],
    "type": "MESOS",
    "volumes": [],
    "docker": {
      "image": "tkrausjr/1221-appdeploy:v2",
      "forcePullImage": false,
      "parameters": []
    }
  },
  "cpus": 1,
  "disk": 0,
  "env": {
    "DOMAIN_NAME": "base_domain",
    "CLUSTER_NAME": "DockerCluster",
    "APP_NAME": "sample",
    "ADMIN_PORT": "7001",
    "ADMIN_PASS": "Welcome1",
    "DOMAIN_HOME": "/u01/oracle/user_projects/domains/base_domain",
    "APP_PKG_FILE": "sample.war",
    "ADMIN_NAME": "weblogic"
  },
  "healthChecks": [
    {
      "gracePeriodSeconds": 300,
      "ignoreHttp1xx": false,
      "intervalSeconds": 25,
      "maxConsecutiveFailures": 3,
      "portIndex": 0,
      "timeoutSeconds": 20,
      "delaySeconds": 15,
      "protocol": "HTTP",
      "path": "/sample"
    }
  ],
  "instances": 1,
  "labels": {
    "HAPROXY_GROUP": "external"
  },
  "maxLaunchDelaySeconds": 3600,
  "mem": 4098,
  "gpus": 0,
  "networks": [
    {
      "mode": "container/bridge"
    }
  ],
  "requirePorts": false,
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  },
  "killSelection": "YOUNGEST_FIRST",
  "unreachableStrategy": {
    "inactiveAfterSeconds": 0,
    "expungeAfterSeconds": 0
  },
  "fetch": [],
  "constraints": []
}